#!/usr/bin/make -f

%:
	dh $@
	dh_installinit --name=openas-firewall -- defaults 20
	dh_installinit --name=openas-rtlogd -- defaults 20
	dh_installinit --name=openas-quarantine-cron -- defaults 60
	dh_installinit --name=openas-quarantine-ng -- defaults 60
	dh_installinit --name=openas-ldap-sync --no-start -- defaults 60
	dh_installinit --name=openas-backend -- defaults 60
	dh_install
	dh_installcron
	dh_install
	dh_installlogrotate
